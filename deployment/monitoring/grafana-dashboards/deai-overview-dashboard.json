{
  "dashboard": {
    "id": null,
    "title": "DeAI Platform Overview",
    "tags": ["deai", "overview", "platform"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "System Health Overview",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "up{job=\"deai-api-gateway\"}",
            "legendFormat": "API Gateway",
            "refId": "A"
          },
          {
            "expr": "up{job=\"deai-web-dashboard\"}",
            "legendFormat": "Web Dashboard", 
            "refId": "B"
          },
          {
            "expr": "up{job=\"postgresql\"}",
            "legendFormat": "Database",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "UP",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Active Nodes",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 3,
          "x": 6,
          "y": 0
        },
        "targets": [
          {
            "expr": "deai_active_nodes_count",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 5
                },
                {
                  "color": "green",
                  "value": 10
                }
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Task Throughput (TPS)",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 3,
          "x": 9,
          "y": 0
        },
        "targets": [
          {
            "expr": "rate(deai_tasks_completed_total[5m])",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 50
                },
                {
                  "color": "green",
                  "value": 100
                }
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Success Rate",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 3,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "expr": "(1 - rate(deai_tasks_failed_total[5m]) / rate(deai_tasks_total[5m])) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 95
                },
                {
                  "color": "green",
                  "value": 99
                }
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "API Response Time",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 3,
          "x": 15,
          "y": 0
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"deai-api-gateway\"}[5m]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 2
                }
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Task Processing Over Time",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "targets": [
          {
            "expr": "rate(deai_tasks_completed_total[5m])",
            "legendFormat": "Completed",
            "refId": "A"
          },
          {
            "expr": "rate(deai_tasks_failed_total[5m])",
            "legendFormat": "Failed",
            "refId": "B"
          },
          {
            "expr": "deai_pending_tasks_count",
            "legendFormat": "Pending",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "color": {
              "mode": "palette-classic"
            },
            "unit": "ops"
          }
        }
      },
      {
        "id": 7,
        "title": "Node Status Distribution",
        "type": "piechart",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 8
        },
        "targets": [
          {
            "expr": "deai_nodes_by_status",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            }
          }
        }
      },
      {
        "id": 8,
        "title": "Token Economics",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        },
        "targets": [
          {
            "expr": "deai_total_supply",
            "legendFormat": "Total Supply",
            "refId": "A"
          },
          {
            "expr": "deai_circulating_supply",
            "legendFormat": "Circulating Supply",
            "refId": "B"
          },
          {
            "expr": "rate(deai_tokens_minted_total[1h])",
            "legendFormat": "Minting Rate (per hour)",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 0,
              "gradientMode": "none"
            },
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 9,
        "title": "Resource Utilization",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 16
        },
        "targets": [
          {
            "expr": "avg(rate(container_cpu_usage_seconds_total{pod=~\"deai-.*\"}[5m])) * 100",
            "legendFormat": "CPU Usage %",
            "refId": "A"
          },
          {
            "expr": "avg(container_memory_usage_bytes{pod=~\"deai-.*\"} / container_spec_memory_limit_bytes{pod=~\"deai-.*\"}) * 100",
            "legendFormat": "Memory Usage %",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 10
            },
            "color": {
              "mode": "palette-classic"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 70
                },
                {
                  "color": "red",
                  "value": 90
                }
              ]
            }
          }
        }
      },
      {
        "id": 10,
        "title": "Error Rate by Component",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        },
        "targets": [
          {
            "expr": "rate(deai_errors_total[5m])",
            "legendFormat": "{{component}} - {{error_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 0
            },
            "color": {
              "mode": "palette-classic"
            },
            "unit": "ops"
          }
        }
      },
      {
        "id": 11,
        "title": "Alert Summary",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 24
        },
        "targets": [
          {
            "expr": "count(ALERTS{alertstate=\"firing\", severity=\"critical\"})",
            "legendFormat": "Critical",
            "refId": "A"
          },
          {
            "expr": "count(ALERTS{alertstate=\"firing\", severity=\"warning\"})",
            "legendFormat": "Warning",
            "refId": "B"
          },
          {
            "expr": "count(ALERTS{alertstate=\"firing\", severity=\"info\"})",
            "legendFormat": "Info",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 3
                }
              ]
            }
          }
        }
      },
      {
        "id": 12,
        "title": "Network Latency",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 32
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{job=\"deai-api-gateway\"}[5m]))",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"deai-api-gateway\"}[5m]))",
            "legendFormat": "P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket{job=\"deai-api-gateway\"}[5m]))",
            "legendFormat": "P99",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 0
            },
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 13,
        "title": "Top Performing Nodes",
        "type": "table",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 32
        },
        "targets": [
          {
            "expr": "topk(10, deai_node_tasks_completed_total)",
            "format": "table",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            }
          }
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "environment",
          "type": "custom",
          "options": [
            {
              "text": "Production",
              "value": "production",
              "selected": true
            },
            {
              "text": "Staging",
              "value": "staging",
              "selected": false
            },
            {
              "text": "Development",
              "value": "development",
              "selected": false
            }
          ]
        },
        {
          "name": "interval",
          "type": "interval",
          "options": [
            {
              "text": "30s",
              "value": "30s",
              "selected": false
            },
            {
              "text": "1m",
              "value": "1m",
              "selected": true
            },
            {
              "text": "5m",
              "value": "5m",
              "selected": false
            },
            {
              "text": "15m",
              "value": "15m",
              "selected": false
            }
          ]
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "prometheus",
          "expr": "changes(up{job=\"deai-api-gateway\"}[1h]) > 0",
          "titleFormat": "Deployment",
          "textFormat": "Service restart detected",
          "iconColor": "blue"
        },
        {
          "name": "Alerts",
          "datasource": "prometheus", 
          "expr": "ALERTS{alertstate=\"firing\"}",
          "titleFormat": "{{alertname}}",
          "textFormat": "{{summary}}",
          "iconColor": "red"
        }
      ]
    }
  }
}